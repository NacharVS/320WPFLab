<Window x:Class="Timer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Timer"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="TimeBlock" HorizontalAlignment="Center" Margin="0,158,0,0" Text="00:00:00" TextWrapping="Wrap" VerticalAlignment="Top" Height="55" Width="196" FontSize="44"/>
        <Button x:Name="Start" Content="Start" HorizontalAlignment="Left" Margin="302,270,0,0" VerticalAlignment="Top" Height="32" Width="62" Click="Start_Click"/>
        <Button x:Name="StartAsync" Content="StartAsync" HorizontalAlignment="Left" Margin="400,270,0,0" VerticalAlignment="Top" Height="32" Width="62" Click="StartAsync_Click"/>
        <TextBox x:Name="TimeBox" HorizontalAlignment="Left" Margin="179,270,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="72" Height="32"/>
    </Grid>
</Window>
